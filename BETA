#include <iostream>
#include <conio.h>
#include <stdio.h>
#include <windows.h>

int main(){
	
	printf("*");
	
	HANDLE hCon;
	hCon = GetStdHandle(STD_OUTPUT_HANDLE);
	COORD dwPos;
	dwPos.X = 4;
	dwPos.Y = 8;
	
	SetConsoleCursorPosition(hCon, dwPos);
	printf("*");
	
	
	
	
	
	
	getch();
	
}
